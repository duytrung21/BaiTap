      (function(){
        $('#rc-form .verify').on('click', function(e) {
        this.disabled = true;
        this.innerHTML = '<i class="fa fa-spinner fa-spin"></i> ' + '送信'
          e.preventDefault();
          grecaptcha.ready(function() {
            grecaptcha.execute('6Lc-usEqAAAAAKQSEMUPT-kHihihSr_5-C-9iSS8', {action:'submit'}).then(function(token) {
              $('#recaptcha-res').remove();
              $('#form-fields').prepend('<input type="hidden" name="recaptcha_response" id="recaptcha-res" value="' + token + '">');
              Rails.fire($("#rc-form")[0], "submit");
            });
