  <script src="/assets/application-18b2423570b09885caa2263ddf518d9ce113328cae3410a7df7dfb318b96c17f.js" data-turbolinks-track="reload"></script>
  <script>
    (function(){
      $(document).ready(function(){
        $('a:not([target])').attr('target', '_parent');
        $(document).on('click', '#cancel-btn', function(e) {
          e.preventDefault();
          $('#confirm-fields').hide().html('');
          $('#form-fields').show();
          setTimeout(function() {
            try {
              window.parent.postMessage( {id: '_bownow_iframe_sid_704975721d89575508ad', height: $('html').outerHeight()}, 'https://www.fc-soft.jp/trial_contact/');
            } catch (e) {}
          },10);
        });
        $(document).on('change', '#form-fields input[type="checkbox"]:not(.hidden)', function () {
          $(this).next().next('input[type="checkbox"]').prop('checked', $(this).prop('checked'));
          $(this).next().next('input[type="checkbox"]').next('input[type="checkbox"]').prop('checked', $(this).prop('checked'));
        });
        $(document).on('change', '#form-fields input[type="radio"]:not(.hidden)', function () {
          $(this).next().next('input[type="radio"]').prop('checked', $(this).prop('checked'));
          $(this).next().next('input[type="radio"]').next('input[type="radio"]').prop('checked', $(this).prop('checked'));
        });
        $(document).on('change', '#form-fields select:not([multiple="multiple"])', function () {
          if ($(this).find('option:selected:not([value=""])').text()) {
            $(this).next('input[type="hidden"]').val($(this).find('option:selected:not([value=""])').text());
            if ($(this).find('option:selected:not([value=""])').val() === ("no_option_" + $(this).find('option:selected:not([value=""])').text())) {
              $(this).next('input[type="hidden"]').next('input[type="hidden"]').val($(this).find('option:selected:not([value=""])').text());
            } else {
              $(this).next('input[type="hidden"]').next('input[type="hidden"]').val($(this).find('option:selected:not([value=""])').text() + '(' + $(this).find('option:selected:not([value=""])').attr('value') + ')');
            }
          } else {
            $(this).next('input[type="hidden"]').val('');
            $(this).next('input[type="hidden"]').next('input[type="hidden"]').val('');
          }
        });
        $(document).on('change', '#form-fields select[multiple="multiple"]', function () {
          var $self = $(this);
          $self.next('.select_multiple_keys').html('');
          $self.next('.select_multiple_keys').next('.select_multiple_key_values').html('');
          $self.find('option:selected').each(function() {
            var $key = $('<input type="hidden" name="' + $self.attr('name').replace('[]', '') + '_key[]" value="">');
            $key.val($(this).text());
            $self.next('.select_multiple_keys').append($key);

            var $key_value = $('<input type="hidden" name="' + $self.attr('name').replace('[]', '') + '_key_value[]" value="">');
            if ($(this).val() === ("no_option_" + $(this).text())) {
              $key_value.val($(this).text());
            } else {
              $key_value.val($(this).text() + '(' + $(this).attr('value') + ')');
            }
            $self.next('.select_multiple_keys').next('.select_multiple_key_values').append($key_value);
          });
        });
        $('#form-fields select').each(function() {
          $(this).trigger('change');
        });
        $(window).one('load', function() {
          setTimeout(function() {
            try {
              window.parent.postMessage( {id: '_bownow_iframe_sid_704975721d89575508ad', height: $('html').outerHeight()}, 'https://www.fc-soft.jp/trial_contact/');
            } catch (e) {}
          },10);
        });
        $(window).one('message', function(e) {
          setTimeout(function() {
            try {
              if (e.originalEvent.data !== 'focus') { return }
              window.parent.postMessage( {id: '_bownow_iframe_sid_704975721d89575508ad', height: $('html').outerHeight()}, 'https://www.fc-soft.jp/trial_contact/');
            } catch (e) {}
          },10);
        });
      });
    }).call(this);
