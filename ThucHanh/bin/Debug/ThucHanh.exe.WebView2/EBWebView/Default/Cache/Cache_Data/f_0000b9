  <script type="text/javascript">
    if('ontouchend' in document) {
      document.write('<script src=\"/assets/jquery.mobile.custom.min-4f41bc4b9d8f6e4a6371a10fae8fb348cdbc9dcafb6715115f5b0229793b272e.js\" data-turbolinks-track=\"reload\"><\/script>');
